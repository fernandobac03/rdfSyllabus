<#TriplesMapC001>
      rr:logicalTable
              [ rr:sqlQuery "SELECT ID_OBJETIVO_URL AS C001_ID FROM DEFAULTDATA WHERE TRANSID = 'RDF_DE_OBJETIVOS_UDA' AND STEPID = 'DATA PRECATCHING' AND FIELD LIKE 'id_objetivo_especifico'"
              ] ;
      rr:subjectMap
              [ rr:class <http://ies.linkeddata.ec/syllabusOntology/vocabulary#Objective> ;
                rr:template "http://ies.linkeddata.ec/uda/item/{C001_ID}"
              ] .

<#TriplesMapA002>
      rr:logicalTable
              [ rr:sqlQuery "SELECT DATA AS A002_DATA, ID_OBJETIVO_URL AS C004_ID FROM DEFAULTDATA WHERE TRANSID = 'RDF_DE_OBJETIVOS_UDA' AND STEPID = 'DATA PRECATCHING' AND FIELD LIKE 'objetivo'"
              ] ;
      rr:predicateObjectMap
              [ rr:objectMap
                        [ rr:column "A002_DATA" ;
                          rr:language "es"
                        ] ;
                rr:predicate rdfs:Label
              ] ;
      rr:subjectMap
              [ rr:template "http://ies.linkeddata.ec/uda/item/{C004_ID}"
              ] .

